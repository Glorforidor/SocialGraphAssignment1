                <span class="s2">&quot;title&quot;</span><span class="o">:</span> <span class="s2">&quot;Challengers of the Unknown (alternate series)&quot;</span><span class="p">,</span>
                <span class="s2">&quot;revisions&quot;</span><span class="o">:</span> <span class="p">[</span>
